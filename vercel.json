{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/blog",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=60"
        }
      ]
    },
    {
      "source": "/blog/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=60"
        }
      ]
    },
    {
      "source": "/snippets",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=60"
        }
      ]
    },
    {
      "source": "/snippets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=60"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/resume",
      "destination": "/sayandey-resume.pdf"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/youtube",
      "destination": "https://www.youtube.com/channel/UCGc7G2cwRBzasx0gCdfz8Lw"
    },
    {
      "source": "/live",
      "destination": "https://www.youtube.com/channel/UCGc7G2cwRBzasx0gCdfz8Lw/live"
    },
    {
      "source": "/newsletter/(.*)",
      "destination": "/newsletter"
    },
    {
      "source": "/blog/2020",
      "destination": "/blog"
    },
    {
      "source": "/blog/2021",
      "destination": "/blog"
    },
    {
      "source": "/blog/2022",
      "destination": "/blog"
    },
    {
      "source": "/blog/resume",
      "destination": "/blog"
    }
  ]
}
